<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <a href='/new'>Add New Message</a>
    <% messages.forEach((item) => { %>
      <p><%= item.user %>: <%= item.text %></p>
      <p><%= item.added %></p><br>
    <% }) %>
  </body>
</html>
